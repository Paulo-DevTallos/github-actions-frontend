{
  "name": "estudo-cicd",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Paulo Sergio <pauloserg861@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "rm -rf build && next build && next export && mv ./out ./build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy:prod": "yarn deploy:staging --prod",
    "deploy:staging": "yarn build && npx vercel ./build --confirm --no-clipboard --token=$VERCEL_TOKEN",
    "deploy:preview": "node ./scripts-ci/pullRequestPreview"
  },
  "dependencies": {
    "next": "^13.1.6",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3"
  }
}
